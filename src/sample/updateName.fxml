<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<DialogPane fx:controller="sample.Model.UpdateNameController"
            xmlns:fx="http://javafx.com/fxml">
    <headerText>
        Insert the updated name for the artist.
    </headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="New name:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <TextField fx:id="name" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
        </GridPane>
    </content>
</DialogPane>